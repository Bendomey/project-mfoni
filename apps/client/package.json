{
  "name": "client",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "description": "Where all users interact with the system.",
  "type": "module",
  "engines": {
    "node": "18",
    "npm": ">=8"
  },
  "scripts": {
    "clean": "rimraf ./node_modules/.cache ./build ./server-build ./public/build",
    "prebuild": "yarn clean && echo All clean âœ¨",
    "build": "run-s build:*",
    "build:remix": "remix build",
    "build:server": "tsx ./scripts/build-server.ts",
    "dev": "remix dev -c \"node ./server/dev.js\" --manual",
    "start": "NODE_ENV=production node ./index.js",
    "test": "jest",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|gql|graphql|mdx|vue)\"",
    "types:check": "tsc -b .",
    "validate": "yarn test --silent -- --watch=false && yarn lint && yarn types:check"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn format",
      "yarn lint"
    ]
  },
  "keywords": [
    "experimental",
    "remix",
    "express"
  ],
  "author": "Domey Benjamin<domeybenjamin1@gmail.com>,Fiifi Arkhurst<fiiarkhurst@gmail.com>,Edward Adjei",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-rekognition": "3.478.0",
    "@aws-sdk/client-s3": "3.478.0",
    "@headlessui/react": "1.7.17",
    "@heroicons/react": "2.1.1",
    "@remix-run/css-bundle": "2.4.1",
    "@remix-run/express": "2.4.1",
    "@remix-run/node": "2.4.1",
    "@remix-run/react": "2.4.1",
    "@remix-run/serve": "2.4.1",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "address": "1.2.2",
    "chalk": "5.3.0",
    "close-with-grace": "1.2.0",
    "crypto-es": "^2.1.0",
    "dotenv": "16.3.1",
    "esbuild": "0.19.3",
    "execa": "8.0.1",
    "express": "4.18.2",
    "framer-motion": "10.15.2",
    "fs-extra": "11.1.1",
    "get-port": "7.0.0",
    "isbot": "3.6.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "9.5.3",
    "react-webcam": "7.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "2.4.1",
    "@types/express": "4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "20.10.5",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@types/react-webcam": "3.0.0",
    "eslint": "8.56.0",
    "eslint-config-kentcdodds": "20.5.0",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "glob": "10.3.4",
    "npm-run-all": "4.1.5",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "0.5.9",
    "rimraf": "5.0.1",
    "tailwindcss": "3.4.0",
    "tsx": "4.6.2",
    "typescript": "5.3.3"
  },
  "resolutions": {
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18"
  }
}
